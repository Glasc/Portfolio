---
import Header from "../components/Header.astro"
import Layout from "../components/layout/Layout.astro"
import Navbar from "../components/navbar/Navbar.astro"
import Profile from "../components/Profile.astro"
import Projects from "../components/projects/Projects.astro"

// its not possible to pass this list to getStaticPaths, so we have to duplicate it
// if  you pass it, you'll get undefined inside getStaticPaths.
const languageList = ["en", "es"] as const
export type Language = (typeof languageList)[number]

export function getStaticPaths() {
  return [
    { params: { languageParam: "en" } },
    { params: { languageParam: "es" } },
  ]
}
---

<Layout>
  <Profile />
  <Navbar />
  <Header />
  <Projects />
</Layout>
